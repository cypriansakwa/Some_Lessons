//Accessing Array Elements with Output
fn main() {
	let a = [1, 2, 3, 4, 5,10,23];
		
	let first = a[0];
	let second = a[1];
	let sixth = a[5];
	let seventh = a[6];
		
	println("First value:");
	println(first);
		
	println("Second value:");
	println(second);
		
	println("Sixth value:");
	println(sixth);
		
	println("Seventh value:");
	println(seventh);
}